RewriteRule ^users/s/([^/\.]+)/?$ index.php?nav=users&sid=$1 [L]
RewriteRule ^punten/d/([^/\.]+)/?$ index.php?nav=punten&did=$1 [L]
RewriteRule ^products/c/([^/\.]+)/?$ index.php?nav=products&cid=$1 [L]
RewriteRule ^products/d/([^/\.]+)/?$ index.php?nav=products&did=$1 [L]
RewriteRule ^products/s/([^/\.]+)/?$ index.php?nav=products&sid=$1 [L]
RewriteRule ^products/n/([^/\.]+)/?$ index.php?nav=products&nid=$1 [L]
RewriteRule ^products/o/([^/\.]+)/?$ index.php?nav=products&oid=$1 [L]
RewriteRule ^orders/c/([^/\.]+)/?$ index.php?nav=orders&cid=$1 [L]
RewriteRule ^orders/d/([^/\.]+)/?$ index.php?nav=orders&did=$1 [L]
RewriteRule ^orders/s/([^/\.]+)/?$ index.php?nav=orders&sid=$1 [L]
